{
  "name": "semester-project-2",
  "version": "1.0.0",
  "type": "module",
  "description": "Auction website for semester project 2 (Noroff).",
  "main": "index.mjs",
  "scripts": {
    "build:css": "sass src/scss:dist/css",
    "build:js": "vite build",
    "build": "npm run build:css && npm run build:js",
    "watch": "sass --watch src/scss:dist/css & vite",
    "start": "npm run build && vite",
    "dev": "npm-run-all --parallel watch start"
  },
  "keywords": [
    "CSS",
    "Frameworks",
    "Bootstrap",
    "SCSS",
    "SASS"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.75.0",
    "vite": "^5.2.8"
  }
}
